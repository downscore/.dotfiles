[
  // Duplicate line/selection: shift-cmd-d
  {
    "key": "shift+cmd+d",
    "command": "-workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled"
  },
  {
    "key": "shift+cmd+d",
    "command": "editor.action.duplicateSelection"
  },
  // Accessibility mode breaks pressing cmd+up to go to the top of the document.
  {
    "key": "cmd+up",
    "command": "-search.action.focusSearchFromResults",
    "when": "accessibilityModeEnabled && searchViewletVisible || firstMatchFocus && searchViewletVisible"
  },
  // Jump to numbered bookmarks.
  {
    "key": "ctrl+1",
    "command": "numberedBookmarks.jumpToBookmark1",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+1",
    "command": "-numberedBookmarks.jumpToBookmark1",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+2",
    "command": "numberedBookmarks.jumpToBookmark2",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+2",
    "command": "-numberedBookmarks.jumpToBookmark2",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+3",
    "command": "numberedBookmarks.jumpToBookmark3",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+3",
    "command": "-numberedBookmarks.jumpToBookmark3",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+4",
    "command": "numberedBookmarks.jumpToBookmark4",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+4",
    "command": "-numberedBookmarks.jumpToBookmark4",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+5",
    "command": "numberedBookmarks.jumpToBookmark5",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+5",
    "command": "-numberedBookmarks.jumpToBookmark5",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+6",
    "command": "numberedBookmarks.jumpToBookmark6",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+6",
    "command": "-numberedBookmarks.jumpToBookmark6",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+7",
    "command": "numberedBookmarks.jumpToBookmark7",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+7",
    "command": "-numberedBookmarks.jumpToBookmark7",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+8",
    "command": "numberedBookmarks.jumpToBookmark8",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+8",
    "command": "-numberedBookmarks.jumpToBookmark8",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+9",
    "command": "numberedBookmarks.jumpToBookmark9",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+9",
    "command": "-numberedBookmarks.jumpToBookmark9",
    "when": "editorTextFocus"
  },
  // Save numbered bookmarks.
  {
    "key": "ctrl+shift+1",
    "command": "numberedBookmarks.toggleBookmark1",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+1",
    "command": "-numberedBookmarks.toggleBookmark1",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+2",
    "command": "numberedBookmarks.toggleBookmark2",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+2",
    "command": "-numberedBookmarks.toggleBookmark2",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+3",
    "command": "numberedBookmarks.toggleBookmark3",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+3",
    "command": "-numberedBookmarks.toggleBookmark3",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+4",
    "command": "numberedBookmarks.toggleBookmark4",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+4",
    "command": "-numberedBookmarks.toggleBookmark4",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+5",
    "command": "numberedBookmarks.toggleBookmark5",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+5",
    "command": "-numberedBookmarks.toggleBookmark5",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+6",
    "command": "numberedBookmarks.toggleBookmark6",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+6",
    "command": "-numberedBookmarks.toggleBookmark6",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+7",
    "command": "numberedBookmarks.toggleBookmark7",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+7",
    "command": "-numberedBookmarks.toggleBookmark7",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+8",
    "command": "numberedBookmarks.toggleBookmark8",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+8",
    "command": "-numberedBookmarks.toggleBookmark8",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+9",
    "command": "numberedBookmarks.toggleBookmark9",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+9",
    "command": "-numberedBookmarks.toggleBookmark9",
    "when": "editorTextFocus"
  },
  // Navigate splits using ctrl-hjkl.
  // Same behavior as tmux and neovim configs.
  {
    "key": "ctrl+k",
    "command": "-deleteAllRight",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.focusAboveGroup"
  },
  {
    "key": "cmd+k cmd+up",
    "command": "-workbench.action.focusAboveGroup"
  },
  {
    "key": "ctrl+j",
    "command": "-editor.action.joinLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.focusBelowGroup"
  },
  {
    "key": "cmd+k cmd+down",
    "command": "-workbench.action.focusBelowGroup"
  },
  {
    "key": "ctrl+h",
    "command": "-deleteLeft",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusLeftGroup"
  },
  {
    "key": "cmd+k cmd+left",
    "command": "-workbench.action.focusLeftGroup"
  },
  {
    "key": "ctrl+l",
    "command": "-workbench.action.chat.newChat",
    "when": "chatIsEnabled && inChat"
  },
  {
    "key": "ctrl+l",
    "command": "-notebook.centerActiveCell",
    "when": "notebookEditorFocused"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.focusRightGroup"
  },
  {
    "key": "cmd+k cmd+right",
    "command": "-workbench.action.focusRightGroup"
  },
  // Split editor with ctrl-\ ctrl-| and ctrl--
  {
    "key": "ctrl+-",
    "command": "-workbench.action.navigateBack",
    "when": "canNavigateBack"
  },
  {
    "key": "ctrl+-",
    "command": "-workbench.action.quickInputBack",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+-",
    "command": "workbench.action.splitEditorDown"
  },
  {
    "key": "cmd+k cmd+\\",
    "command": "-workbench.action.splitEditorDown"
  },
  {
    "key": "ctrl+\\",
    "command": "workbench.action.splitEditorRight"
  },
  {
    "key": "ctrl+shift+\\",
    "command": "workbench.action.splitEditorRight"
  },
  {
    "key": "cmd+k cmd+\\",
    "command": "-workbench.action.splitEditorRight"
  },
  // Go to last split using ctrl-p.
  {
    "key": "ctrl+p",
    "command": "-workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+p",
    "command": "-list.focusUp",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused"
  },
  {
    "key": "ctrl+p",
    "command": "-selectPrevCodeAction",
    "when": "codeActionMenuVisible"
  },
  {
    "key": "ctrl+p",
    "command": "-selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
  },
  {
    "key": "ctrl+p",
    "command": "-showPrevParameterHint",
    "when": "editorFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
  },
  {
    "key": "ctrl+p",
    "command": "-cursorUp",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.focusPreviousGroup"
  },
  // Focus active editor using ctrl-a.
  {
    "key": "ctrl+a",
    "command": "-cursorLineStart",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+a",
    "command": "workbench.action.focusActiveEditorGroup"
  },
  // Toggle maximizing split using ctrl-z.
  {
    "key": "ctrl+z",
    "command": "workbench.action.toggleMaximizeEditorGroup",
    "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups"
  },
  // Search local file symbols using ctrl-f.
  {
    "key": "ctrl+f",
    "command": "-cursorRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+f",
    "command": "workbench.action.gotoSymbol",
    "when": "!accessibilityHelpIsShown && !accessibleViewIsShown"
  },
  // Navigate to next/previous changes with ctrl-[ and ctrl-].
  {
    "key": "ctrl+]",
    "command": "workbench.action.editor.nextChange",
    "when": "editorTextFocus && !textCompareEditorActive"
  },
  {
    "key": "ctrl+[",
    "command": "workbench.action.editor.previousChange",
    "when": "editorTextFocus && !textCompareEditorActive"
  },
  {
    "key": "ctrl+[",
    "command": "workbench.action.compareEditor.previousChange",
    "when": "textCompareEditorVisible"
  },
  {
    "key": "ctrl+]",
    "command": "workbench.action.compareEditor.nextChange",
    "when": "textCompareEditorVisible"
  },
  // Close split using ctrl-x.
  {
    "key": "ctrl+x",
    "command": "workbench.action.closeEditorsAndGroup"
  },
  // Open splits using ctrl-alt-j/l.
  {
    "key": "ctrl+alt+j",
    "command": "-notebook.cell.joinBelow",
    "when": "notebookEditorFocused"
  },
  {
    "key": "ctrl+alt+j",
    "command": "workbench.action.splitEditorDown"
  },
  {
    "key": "ctrl+alt+l",
    "command": "workbench.action.splitEditorRight"
  },
  // Go to next/previous change with ctrl-alt-n/p.
  {
    "key": "ctrl+alt+n",
    "command": "-list.focusAnyDown",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused"
  },
  {
    "key": "ctrl+alt+p",
    "command": "-list.focusAnyUp",
    "when": "listFocus && !inputFocus && !treestickyScrollFocused"
  },
  {
    "key": "ctrl+alt+n",
    "command": "workbench.action.compareEditor.nextChange",
    "when": "textCompareEditorVisible"
  },
  {
    "key": "ctrl+alt+n",
    "command": "workbench.action.editor.nextChange",
    "when": "editorTextFocus && !textCompareEditorActive"
  },
  {
    "key": "ctrl+alt+p",
    "command": "workbench.action.compareEditor.previousChange",
    "when": "textCompareEditorVisible"
  },
  {
    "key": "ctrl+alt+p",
    "command": "workbench.action.editor.previousChange",
    "when": "editorTextFocus && !textCompareEditorActive"
  },
  // Copy active file path using ctrl-alt-c.
  {
    "key": "ctrl+alt+c",
    "command": "copyFilePath"
  },
  // Go to definition with ctrl-alt-k.
  {
    "key": "ctrl+alt+k",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus"
  },
  // Go to bracket with ctrl-alt-a.
  {
    "key": "ctrl+alt+a",
    "command": "editor.action.jumpToBracket",
    "when": "editorTextFocus"
  },
  // Run all tests with ctrl-alt-t.
  {
    "key": "ctrl+alt+t",
    "command": "testing.runAll"
  },
  // Go to references with ctrl-alt-r.
  {
    "key": "ctrl+alt+r",
    "command": "editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "ctrl+alt+r",
    "command": "-workbench.action.terminal.runRecentCommand",
    "when": "terminalFocus && terminalHasBeenCreated && !accessibilityModeEnabled || terminalFocus && terminalProcessSupported && !accessibilityModeEnabled"
  },
  {
    "key": "ctrl+alt+r",
    "command": "-workbench.action.terminal.sendSequence",
    "when": "accessibilityModeEnabled && terminalFocus"
  },
  // Re-run last test with ctrl-alt-x.
  {
    "key": "ctrl+alt+x",
    "command": "testing.reRunLastRun"
  },
  // Move editor between splits with ctrl-shift-hjkl.
  {
    "key": "ctrl+shift+k",
    "command": "workbench.action.moveEditorToAboveGroup"
  },
  {
    "key": "ctrl+shift+j",
    "command": "workbench.action.moveEditorToBelowGroup"
  },
  {
    "key": "ctrl+shift+h",
    "command": "workbench.action.moveEditorToLeftGroup"
  },
  {
    "key": "ctrl+shift+l",
    "command": "workbench.action.moveEditorToRightGroup"
  },
  // Go to next/previous problem with ctrl-shift-n/p.
  {
    "key": "ctrl+shift+n",
    "command": "editor.action.marker.nextInFiles",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+shift+p",
    "command": "editor.action.marker.prevInFiles",
    "when": "editorFocus"
  },
  // Insert a line below with ctrl-alt-o.
  {
    "key": "ctrl+alt+o",
    "command": "editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
  },
  // Source control commands using ctrl-shift.
  {
    "key": "ctrl+shift+c",
    "command": "git.checkout"
  },
  {
    "key": "ctrl+shift+s",
    "command": "git.pull"
  },
  {
    "key": "ctrl+shift+u",
    "command": "git.push"
  },
  {
    "key": "ctrl+shift+q",
    "command": "-workbench.action.quickOpenNavigatePreviousInViewPicker",
    "when": "inQuickOpen && inViewsPicker"
  },
  {
    "key": "ctrl+shift+q",
    "command": "git.revealFileInOS.mac"
  },
  {
    "key": "ctrl+shift+b",
    "command": "git.rebase"
  },
  // Toggle block comment with ctrl-alt-/.
  {
    "key": "ctrl+alt+/",
    "command": "editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
  },
  // Shortcuts for working with anchors with ctrl-shift-cmd.
  {
    "key": "ctrl+shift+cmd+m",
    "command": "editor.action.setSelectionAnchor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+cmd+v",
    "command": "editor.action.selectFromAnchorToCursor",
    "when": "editorTextFocus && selectionAnchorSet"
  },
  {
    "key": "ctrl+shift+cmd+j",
    "command": "editor.action.goToSelectionAnchor"
  },
  // Select to bracket with ctrl-shift-cmd-a.
  {
    "key": "ctrl+shift+cmd+a",
    "command": "editor.action.selectToBracket"
  },
  // Remove brackets with ctrl-shift-cmd-d.
  {
    "key": "ctrl+shift+cmd+d",
    "command": "editor.action.removeBrackets",
    "when": "editorTextFocus"
  },
  // Insert a line above with ctrl-shift-cmd-o.
  {
    "key": "ctrl+shift+cmd+o",
    "command": "editor.action.insertLineBefore",
    "when": "editorTextFocus && !editorReadonly"
  },
  // Shortcuts for changing text case with ctrl-shift-cmd.
  {
    "key": "ctrl+shift+cmd+c",
    "command": "editor.action.transformToCamelcase"
  },
  {
    "key": "ctrl+shift+cmd+p",
    "command": "editor.action.transformToPascalcase"
  },
  {
    "key": "ctrl+shift+cmd+s",
    "command": "editor.action.transformToSnakecase"
  },
  {
    "key": "ctrl+shift+cmd+k",
    "command": "editor.action.transformToKebabcase"
  },
  {
    "key": "ctrl+shift+cmd+l",
    "command": "editor.action.transformToLowercase"
  },
  {
    "key": "ctrl+shift+cmd+t",
    "command": "editor.action.transformToTitlecase"
  },
  {
    "key": "ctrl+shift+cmd+u",
    "command": "editor.action.transformToUppercase"
  },
  // Make all splits the same size with ctrl-alt-m.
  {
    "key": "ctrl+alt+m",
    "command": "workbench.action.evenEditorWidths"
  },
  // Join lines with cmd-shift-j.
  {
    "key": "shift+cmd+j",
    "command": "-workbench.action.search.toggleQueryDetails",
    "when": "inSearchEditor || searchViewletFocus"
  },
  {
    "key": "shift+cmd+j",
    "command": "editor.action.joinLines"
  },
]
